{% extends 'base.html.twig' %}

{% block title %}Structure index{% endblock %}

{% block body %}
    <h1>Structure index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Slug</th>
                <th>RespFirstName</th>
                <th>RespLastName</th>
                <th>Address</th>
                <th>Complement</th>
                <th>Zipcode</th>
                <th>City</th>
                <th>Siret</th>
                <th>Ape</th>
                <th>TvaNumber</th>
                <th>UrlWeb</th>
                <th>UrlFacebook</th>
                <th>UrlInstagram</th>
                <th>UrlLinkedin</th>
                <th>EmailStruct</th>
                <th>PhoneDesk</th>
                <th>PhoneGsm</th>
                <th>FirstActivity</th>
                <th>SecondActivity</th>
                <th>ProjectDev</th>
                <th>CreatedAt</th>
                <th>UpdatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for structure in structures %}
            <tr>
                <td>{{ structure.id }}</td>
                <td>{{ structure.name }}</td>
                <td>{{ structure.slug }}</td>
                <td>{{ structure.respFirstName }}</td>
                <td>{{ structure.respLastName }}</td>
                <td>{{ structure.address }}</td>
                <td>{{ structure.complement }}</td>
                <td>{{ structure.zipcode }}</td>
                <td>{{ structure.city }}</td>
                <td>{{ structure.siret }}</td>
                <td>{{ structure.ape }}</td>
                <td>{{ structure.tvaNumber }}</td>
                <td>{{ structure.urlWeb }}</td>
                <td>{{ structure.urlFacebook }}</td>
                <td>{{ structure.urlInstagram }}</td>
                <td>{{ structure.urlLinkedin }}</td>
                <td>{{ structure.EmailStruct }}</td>
                <td>{{ structure.phoneDesk }}</td>
                <td>{{ structure.phoneGsm }}</td>
                <td>{{ structure.FirstActivity }}</td>
                <td>{{ structure.secondActivity }}</td>
                <td>{{ structure.projectDev }}</td>
                <td>{{ structure.createdAt ? structure.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ structure.updatedAt ? structure.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('admin_structure_show', {'id': structure.id}) }}">show</a>
                    <a href="{{ path('admin_structure_edit', {'id': structure.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="25">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('admin_structure_new') }}">Create new</a>
{% endblock %}
