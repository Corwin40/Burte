    <h4>Section index</h4>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Title</th>
                <th>Slug</th>
                <th>Description</th>
                <th>IdName</th>
                <th>NameName</th>
                <th>CreateAt</th>
                <th>UpdateAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for section in sections %}
            <tr>
                <td>{{ section.id }}</td>
                <td>{{ section.title }}</td>
                <td>{{ section.slug }}</td>
                <td>{{ section.description }}</td>
                <td>{{ section.idName }}</td>
                <td>{{ section.nameName }}</td>
                <td>{{ section.createAt ? section.createAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ section.updateAt ? section.updateAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('webapp_section_show', {'id': section.id}) }}">show</a>
                    <a href="{{ path('webapp_section_edit', {'id': section.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="9">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('webapp_section_new') }}">Create new</a>

